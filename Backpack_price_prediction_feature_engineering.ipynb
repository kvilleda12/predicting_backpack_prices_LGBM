      "source": [
        "#these features are for makign weight range features based on brand, material, size, style,\n",
        "\n",
        "\n",
        "weight_stats_df = con.execute (\n",
        "    \"\"\"CREATE OR REPLACE VIEW feat_weight_stats AS\n",
        "SELECT\n",
        "  id,\n",
        "\n",
        "  -- Brand stats\n",
        "  AVG(wc)  OVER (PARTITION BY brand)    AS brand_weight_mean,\n",
        "  (MAX(wc) OVER (PARTITION BY brand) - MIN(wc) OVER (PARTITION BY brand)) AS brand_weight_range,\n",
        "\n",
        "  -- Material stats\n",
        "  AVG(wc)  OVER (PARTITION BY material) AS material_weight_mean,\n",
        "  (MAX(wc) OVER (PARTITION BY material) - MIN(wc) OVER (PARTITION BY material)) AS material_weight_range,\n",
        "\n",
        "  -- Size stats\n",
        "  AVG(wc)  OVER (PARTITION BY size)     AS size_weight_mean,\n",
        "  (MAX(wc) OVER (PARTITION BY size) - MIN(wc) OVER (PARTITION BY size))    AS size_weight_range,\n",
        "\n",
        "  -- Style stats\n",
        "  AVG(wc)  OVER (PARTITION BY style)    AS style_weight_mean,\n",
        "  (MAX(wc) OVER (PARTITION BY style) - MIN(wc) OVER (PARTITION BY style))  AS style_weight_range,\n",
        "\n",
        "  -- Ratios vs group means (avoid /0)\n",
        "  wc / NULLIF(AVG(wc) OVER (PARTITION BY brand),    0) AS weight_vs_brand_mean,\n",
        "  wc / NULLIF(AVG(wc) OVER (PARTITION BY material), 0) AS weight_vs_material_mean,\n",
        "  wc / NULLIF(AVG(wc) OVER (PARTITION BY size),     0) AS weight_vs_size_mean,\n",
        "  wc / NULLIF(AVG(wc) OVER (PARTITION BY style),    0) AS weight_vs_style_mean\n",
        "FROM df; \"\"\"\n",
        ")"
      ]
    },
