      "source": [
        "import duckdb\n",
        "\n",
        "TRAIN_SMALL = \"train.csv\"\n",
        "TRAIN_BIG   = \"training_extra.csv\"\n",
        "TEST_PATH   = \"test.csv\"\n",
        "\n",
        "con = duckdb.connect(database=\":memory:\", read_only=False)\n",
        "\n",
        "# Create physical DuckDB tables directly from CSVs\n",
        "con.execute(f\"\"\"\n",
        "  CREATE OR REPLACE TABLE train AS\n",
        "  SELECT * FROM read_csv_auto('{TRAIN_SMALL}')\n",
        "  UNION ALL BY NAME\n",
        "  SELECT * FROM read_csv_auto('{TRAIN_BIG}');\n",
        "\"\"\")\n",
        "\n",
        "con.execute(f\"\"\"\n",
        "  CREATE OR REPLACE TABLE test AS\n",
        "  SELECT * FROM read_csv_auto('{TEST_PATH}');\n",
        "\"\"\")\n",
        "\n",
        "# If/when you need pandas DataFrames:\n",
        "df = con.table(\"train\").df()\n",
        "test_df = con.table(\"test\").df()\n",
        "\n",
        "print(f\"train rows: {len(df):,} | test rows: {len(test_df):,}\")"
      ]
    },
