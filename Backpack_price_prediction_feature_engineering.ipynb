        "final_table = con.execute( \"\"\"\n",
        "  CREATE OR REPLACE TABLE df_all_features AS\n",
        "SELECT\n",
        "  d.*,\n",
        "  fi.brand_size, fi.brand_material, fi.material_size,\n",
        "  ws.brand_weight_mean, ws.brand_weight_range,\n",
        "  ws.material_weight_mean, ws.material_weight_range,\n",
        "  ws.size_weight_mean, ws.size_weight_range,\n",
        "  ws.style_weight_mean, ws.style_weight_range,\n",
        "  ws.weight_vs_brand_mean, ws.weight_vs_material_mean,\n",
        "  ws.weight_vs_size_mean,  ws.weight_vs_style_mean,\n",
        "  cm.brand_total_percent, cm.material_total_percent,\n",
        "  cm.size_total_percent,   cm.style_total_percent,\n",
        "  cm.material_share_within_brand, cm.style_share_within_brand, cm.size_share_within_brand,\n",
        "  cm.is_light, cm.is_medium, cm.is_heavy,\n",
        "  cm.falls_into_global_weight_majority,\n",
        "  cm.falls_into_brand_weight_majority,\n",
        "  cm.falls_into_style_weight_majority,\n",
        "  cm.falls_into_size_weight_majority,\n",
        "  cm.is_above_global_mean_weight,\n",
        "  cm.is_above_brand_mean_weight,\n",
        "  cm.is_above_style_mean_weight,\n",
        "  cm.is_above_size_mean_weight\n",
        "FROM df AS d\n",
        "LEFT JOIN feat_interactions   fi USING (id)\n",
        "LEFT JOIN feat_weight_stats   ws USING (id)\n",
        "LEFT JOIN feat_counts_majority cm USING (id);\n",
        "\n",
        "\"\"\")"
      ]
