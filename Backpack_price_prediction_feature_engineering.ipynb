      "source": [
        "con.execute(\"\"\"\n",
        "CREATE OR REPLACE TABLE te_price_feats AS\n",
        "WITH base AS (\n",
        "  SELECT\n",
        "    id, Price, brand, material, size, style, waterproof,\n",
        "    CAST(ABS(HASH(id)) % 3 AS INTEGER) AS fold_id\n",
        "  FROM df_all_features\n",
        "),\n",
        "gm AS (SELECT AVG(Price) AS global_mean FROM base),\n",
        "\n",
        "-- Totals per group (over ALL folds)\n",
        "brand_tot AS     (SELECT brand,     SUM(Price) AS s, COUNT(*) AS c FROM base GROUP BY brand),\n",
        "material_tot AS  (SELECT material,  SUM(Price) AS s, COUNT(*) AS c FROM base GROUP BY material),\n",
        "size_tot AS      (SELECT size,      SUM(Price) AS s, COUNT(*) AS c FROM base GROUP BY size),\n",
        "style_tot AS     (SELECT style,     SUM(Price) AS s, COUNT(*) AS c FROM base GROUP BY style),\n",
        "wp_tot AS        (SELECT waterproof,SUM(Price) AS s, COUNT(*) AS c FROM base GROUP BY waterproof),\n",
        "bm_tot AS        (SELECT brand,material, SUM(Price) AS s, COUNT(*) AS c FROM base GROUP BY brand,material),\n",
        "bs_tot AS        (SELECT brand,size,     SUM(Price) AS s, COUNT(*) AS c FROM base GROUP BY brand,size),\n",
        "ms_tot AS        (SELECT material,size,  SUM(Price) AS s, COUNT(*) AS c FROM base GROUP BY material,size),\n",
        "\n",
        "-- Per-fold totals (the part we will subtract out)\n",
        "brand_fold AS     (SELECT brand,     fold_id, SUM(Price) AS s, COUNT(*) AS c FROM base GROUP BY brand,fold_id),\n",
        "material_fold AS  (SELECT material,  fold_id, SUM(Price) AS s, COUNT(*) AS c FROM base GROUP BY material,fold_id),\n",
        "size_fold AS      (SELECT size,      fold_id, SUM(Price) AS s, COUNT(*) AS c FROM base GROUP BY size,fold_id),\n",
        "style_fold AS     (SELECT style,     fold_id, SUM(Price) AS s, COUNT(*) AS c FROM base GROUP BY style,fold_id),\n",
        "wp_fold AS        (SELECT waterproof,fold_id, SUM(Price) AS s, COUNT(*) AS c FROM base GROUP BY waterproof,fold_id),\n",
        "bm_fold AS        (SELECT brand,material, fold_id, SUM(Price) AS s, COUNT(*) AS c FROM base GROUP BY brand,material,fold_id),\n",
        "bs_fold AS        (SELECT brand,size,     fold_id, SUM(Price) AS s, COUNT(*) AS c FROM base GROUP BY brand,size,fold_id),\n",
        "ms_fold AS        (SELECT material,size,  fold_id, SUM(Price) AS s, COUNT(*) AS c FROM base GROUP BY material,size,fold_id),\n",
        "\n",
        "alpha AS (SELECT 20.0 AS a),  -- smoothing strength\n",
        "\n",
        "-- Compute OOF TEs for every row by subtracting \"own fold\" stats from group totals\n",
        "calc AS (\n",
        "  SELECT\n",
        "    b.id, b.fold_id, gm.global_mean, a.a,\n",
        "\n",
        "    -- brand\n",
        "    ((bt.s - COALESCE(bf.s,0)) + a.a*gm.global_mean) / NULLIF((bt.c - COALESCE(bf.c,0)) + a.a, 0) AS brand_price_te,\n",
        "    (bt.c - COALESCE(bf.c,0))                                                              AS brand_price_count_te,\n",
        "\n",
        "    -- material\n",
        "    ((mt.s - COALESCE(mf.s,0)) + a.a*gm.global_mean) / NULLIF((mt.c - COALESCE(mf.c,0)) + a.a, 0) AS material_price_te,\n",
        "\n",
        "    -- size\n",
        "    ((zt.s - COALESCE(zf.s,0)) + a.a*gm.global_mean) / NULLIF((zt.c - COALESCE(zf.c,0)) + a.a, 0) AS size_price_te,\n",
        "\n",
        "    -- style\n",
        "    ((stt.s - COALESCE(stf.s,0)) + a.a*gm.global_mean) / NULLIF((stt.c - COALESCE(stf.c,0)) + a.a, 0) AS style_price_te,\n",
        "\n",
        "    -- waterproof\n",
        "    ((wpt.s - COALESCE(wpf.s,0)) + a.a*gm.global_mean) / NULLIF((wpt.c - COALESCE(wpf.c,0)) + a.a, 0) AS waterproof_price_te,\n",
        "\n",
        "    -- brand Ã— material\n",
        "    ((bmt.s - COALESCE(bmf.s,0)) + a.a*gm.global_mean) / NULLIF((bmt.c - COALESCE(bmf.c,0)) + a.a, 0) AS brand_material_price_te,\n",
        "    (bmt.c - COALESCE(bmf.c,0))                                                                         AS brand_material_price_count_te,\n",
        "\n",
        "    -- brand Ã— size\n",
        "    ((bst.s - COALESCE(bsf.s,0)) + a.a*gm.global_mean) / NULLIF((bst.c - COALESCE(bsf.c,0)) + a.a, 0) AS brand_size_price_te,\n",
        "\n",
        "    -- material Ã— size\n",
        "    ((mst.s - COALESCE(msf.s,0)) + a.a*gm.global_mean) / NULLIF((mst.c - COALESCE(msf.c,0)) + a.a, 0) AS material_size_price_te\n",
        "\n",
        "  FROM base b\n",
        "  CROSS JOIN gm\n",
        "  CROSS JOIN alpha a\n",
        "\n",
        "  LEFT JOIN brand_tot     bt ON b.brand     = bt.brand\n",
        "  LEFT JOIN brand_fold    bf ON b.brand     = bf.brand     AND b.fold_id = bf.fold_id\n",
        "\n",
        "  LEFT JOIN material_tot  mt ON b.material  = mt.material\n",
        "  LEFT JOIN material_fold mf ON b.material  = mf.material  AND b.fold_id = mf.fold_id\n",
        "\n",
        "  LEFT JOIN size_tot      zt ON b.size      = zt.size\n",
        "  LEFT JOIN size_fold     zf ON b.size      = zf.size      AND b.fold_id = zf.fold_id\n",
        "\n",
        "  LEFT JOIN style_tot     stt ON b.style    = stt.style\n",
        "  LEFT JOIN style_fold    stf ON b.style    = stf.style    AND b.fold_id = stf.fold_id\n",
        "\n",
        "  LEFT JOIN wp_tot        wpt ON b.waterproof = wpt.waterproof\n",
        "  LEFT JOIN wp_fold       wpf ON b.waterproof = wpf.waterproof AND b.fold_id = wpf.fold_id\n",
        "\n",
        "  LEFT JOIN bm_tot        bmt ON b.brand = bmt.brand AND b.material = bmt.material\n",
        "  LEFT JOIN bm_fold       bmf ON b.brand = bmf.brand AND b.material = bmf.material AND b.fold_id = bmf.fold_id\n",
        "\n",
        "  LEFT JOIN bs_tot        bst ON b.brand = bst.brand AND b.size = bst.size\n",
        "  LEFT JOIN bs_fold       bsf ON b.brand = bsf.brand AND b.size = bsf.size AND b.fold_id = bsf.fold_id\n",
        "\n",
        "  LEFT JOIN ms_tot        mst ON b.material = mst.material AND b.size = mst.size\n",
        "  LEFT JOIN ms_fold       msf ON b.material = msf.material AND b.size = msf.size AND b.fold_id = msf.fold_id\n",
        ")\n",
        "SELECT * EXCLUDE (fold_id, global_mean, a) FROM calc;\n",
        "\n",
        "\"\"\")"
      ]
    },
