      "source": [
        "#these features are counts for global statistics. Basically going to see what percent of demand is there for each backpack\n",
        "#total_count brand/ total count of backpaks  = percentage of total backpacks by that brand\n",
        "#total_count material/ total_backpacks\n",
        "#total_style/ttoal_backpacks\n",
        "#total size/total_backpacks\n",
        "\n",
        "#for some brand specfici ones to find out whether they specialize in a certain one or not\n",
        "#brand_material_count/ total_brand_count\n",
        "#brand_style_count/total_brand_count\n",
        "#brand_size_count/ total_brand_count\n",
        "\n",
        "#these are features to find out if they fall into majorites. If the percent from those other ones are greater then a certain percent\n",
        "#then we will make a categorical coliumn 1 or 0 for falls in majority. Then also for falls in majority of brand\n",
        "#falls into global weight majority (1 or 0)\n",
        "#falls into brand weight majority\n",
        "#falls into style weight majority\n",
        "#falls into size weight maority\n",
        "\n",
        "\n",
        "df_more_cats = con.execute( \"\"\"\n",
        " CREATE OR REPLACE VIEW feat_counts_majority AS\n",
        "WITH base AS (\n",
        "  SELECT\n",
        "    id, brand, material, size, style, wc,\n",
        "    CASE\n",
        "      WHEN wc < 14 THEN 'light'\n",
        "      WHEN wc > 14 AND wc < 22 THEN 'medium'\n",
        "      ELSE 'heavy'\n",
        "    END AS weight_bucket\n",
        "  FROM df\n",
        "),\n",
        "counts AS (\n",
        "  SELECT\n",
        "    *,\n",
        "    COUNT(*) OVER ()                                AS total_cnt,\n",
        "    COUNT(*) OVER (PARTITION BY brand)              AS brand_cnt,\n",
        "    COUNT(*) OVER (PARTITION BY material)           AS material_cnt,\n",
        "    COUNT(*) OVER (PARTITION BY size)               AS size_cnt,\n",
        "    COUNT(*) OVER (PARTITION BY style)              AS style_cnt,\n",
        "    COUNT(*) OVER (PARTITION BY brand, material)    AS brand_material_cnt,\n",
        "    COUNT(*) OVER (PARTITION BY brand, style)       AS brand_style_cnt,\n",
        "    COUNT(*) OVER (PARTITION BY brand, size)        AS brand_size_cnt,\n",
        "    COUNT(*) OVER (PARTITION BY weight_bucket)      AS bucket_cnt,\n",
        "    COUNT(*) OVER (PARTITION BY brand,  weight_bucket) AS brand_bucket_cnt,\n",
        "    COUNT(*) OVER (PARTITION BY style,  weight_bucket) AS style_bucket_cnt,\n",
        "    COUNT(*) OVER (PARTITION BY size,   weight_bucket) AS size_bucket_cnt\n",
        "  FROM base\n",
        "),\n",
        "majority AS (\n",
        "  SELECT\n",
        "    *,\n",
        "    MAX(bucket_cnt)       OVER ()                   AS global_bucket_max,\n",
        "    MAX(brand_bucket_cnt) OVER (PARTITION BY brand) AS brand_bucket_max,\n",
        "    MAX(style_bucket_cnt) OVER (PARTITION BY style) AS style_bucket_max,\n",
        "    MAX(size_bucket_cnt)  OVER (PARTITION BY size)  AS size_bucket_max\n",
        "  FROM counts\n",
        ")\n",
        "SELECT\n",
        "  m.id,\n",
        "  -- % of total per category\n",
        "  1.0 * brand_cnt    / total_cnt AS brand_total_percent,\n",
        "  1.0 * material_cnt / total_cnt AS material_total_percent,\n",
        "  1.0 * size_cnt     / total_cnt AS size_total_percent,\n",
        "  1.0 * style_cnt    / total_cnt AS style_total_percent,\n",
        "\n",
        "  -- shares within brand\n",
        "  1.0 * brand_material_cnt / NULLIF(brand_cnt, 0) AS material_share_within_brand,\n",
        "  1.0 * brand_style_cnt    / NULLIF(brand_cnt, 0) AS style_share_within_brand,\n",
        "  1.0 * brand_size_cnt     / NULLIF(brand_cnt, 0) AS size_share_within_brand,\n",
        "\n",
        "  -- simple weight flags\n",
        "  CASE WHEN wc < 14                 THEN 1 ELSE 0 END AS is_light,\n",
        "  CASE WHEN wc > 14 AND wc < 22     THEN 1 ELSE 0 END AS is_medium,\n",
        "  CASE WHEN wc >= 22                THEN 1 ELSE 0 END AS is_heavy,\n",
        "\n",
        "  -- â€œfalls into majority weight bucketâ€\n",
        "  CASE WHEN bucket_cnt       = global_bucket_max THEN 1 ELSE 0 END AS falls_into_global_weight_majority,\n",
        "  CASE WHEN brand_bucket_cnt = brand_bucket_max  THEN 1 ELSE 0 END AS falls_into_brand_weight_majority,\n",
        "  CASE WHEN style_bucket_cnt = style_bucket_max  THEN 1 ELSE 0 END AS falls_into_style_weight_majority,\n",
        "  CASE WHEN size_bucket_cnt  = size_bucket_max   THEN 1 ELSE 0 END AS falls_into_size_weight_majority,\n",
        "\n",
        "  -- above-mean weight flags (non-leaky)\n",
        "  CASE WHEN wc >= AVG(wc) OVER ()                   THEN 1 ELSE 0 END AS is_above_global_mean_weight,\n",
        "  CASE WHEN wc >= AVG(wc) OVER (PARTITION BY brand) THEN 1 ELSE 0 END AS is_above_brand_mean_weight,\n",
        "  CASE WHEN wc >= AVG(wc) OVER (PARTITION BY style) THEN 1 ELSE 0 END AS is_above_style_mean_weight,\n",
        "  CASE WHEN wc >= AVG(wc) OVER (PARTITION BY size)  THEN 1 ELSE 0 END AS is_above_size_mean_weight\n",
        "FROM majority AS m;\n",
        "\n",
        "\"\"\")\n",
        "\n"
      ]
    },
